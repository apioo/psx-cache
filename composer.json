{
  "name": "psx/cache",
  "description": "PSR-6 and PSR-16 implementation using the doctrine cache system",
  "keywords": ["cache", "doctrine", "psr-6", "psr-16"],
  "homepage": "http://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com/"
    }
  ],
  "require": {
    "php": ">=5.5",
    "doctrine/cache": "^1.6",
    "psr/cache": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Cache\\Tests\\": "tests/"
    }
  },
  "provide": {
    "psr/cache-implementation": "1.0",
    "psr/simple-cache-implementation": "1.0"
  }
}
